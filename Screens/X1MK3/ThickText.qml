import QtQuick 2.0

Text {
    font {
        family: "UpheavalX1"
        weight: 500
        pixelSize: 16
        letterSpacing: 1
        preferShaping: false
        kerning: false
        capitalization: Font.AllUppercase
        hintingPreference: Font.PreferFullHinting
    }
    color: "white"
    antialiasing: false
    renderType: Text.NativeRendering
    verticalAlignment: Text.AlignVCenter
    horizontalAlignment: Text.AlignLeft
}